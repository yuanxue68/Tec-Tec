<div>
  <%=link_to "Place Bid", new_auction_bid_path(@auction), class:"btn btn-success pull-right"%>
  <h5>Auction Name</h5>
  <%= @auction.name%>
  <h5>Description</h5>
  <%= @auction.description%>
  <h5>Course Code</h5>
  <%= @auction.course_code%>
  <h5>School</h5>
  <%= @auction.school%>
  <h5>Start Time</h5>
  <%= @auction.start_time.strftime("%Y-%m-%d %H-%M-%S")%>
  <h5> End Time</h5>
  <%= @auction.end_time.strftime("%Y-%m-%d %H-%M-%S")%>
</div>
